{
  "name": "nnecec-luoo",
  "productName": "nnecec-luoo",
  "description": "nnecec's luoo music player.",
  "version": "0.1.0",
  "private": false,
  "author": "nnecec <nnecec@outlook.com>",
  "copyright": "Â© 2018, nnecec inc.",
  "homepage": "http://nnecec.cc",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "cross-env NODE_ENV=test webpack --config=build/webpack.unit.config.js --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "cross-env NODE_ENV=test webpack --config=build/webpack.app.config.js --display=none && webpack --config=build/webpack.e2e.config.js --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "cross-env NODE_ENV=development node build/start.js",
    "release": "cross-env NODE_ENV=production webpack --config=build/webpack.app.config.js && electron-builder"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "api": "^1.0.0",
    "axios": "^0.18.0",
    "chai": "^4.2.0",
    "components": "^0.1.0",
    "electron-json-storage": "^4.1.4",
    "env": "0.0.2",
    "fs-jetpack": "^2.2.0",
    "mobx": "^5.6.0",
    "mobx-react": "^5.3.6",
    "mobx-react-router": "^4.0.5",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-feather": "^1.1.4",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.0.3",
    "utils": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0-beta.42",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "electron": "^3.0.8",
    "electron-builder": "^20.33.2",
    "electron-reload": "^1.2.5",
    "enzyme": "^3.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^23.6.0",
    "node-sass": "^4.10.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.9",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
